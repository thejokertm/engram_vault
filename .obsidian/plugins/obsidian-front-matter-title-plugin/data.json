{
  "templates": [
    "{{sources}} {{aliases}}"
  ],
  "managers": {
    "explorer": true,
    "graph": true,
    "header": false,
    "quick_switcher": true
  },
  "rules": {
    "paths": {
      "mode": "black",
      "values": []
    },
    "delimiter": {
      "enabled": false,
      "value": ""
    }
  },
  "debug": false,
  "boot": {
    "delay": 1000
  },
  "features": {
    "explorer:sort": {
      "enabled": true
    }
  }
}